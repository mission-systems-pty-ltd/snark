invert[0]="cat test.bin | cv-cat color-map=jet | cv-cat color-map=jet,invert | csv-paste '-;binary=ub' 'test.bin;binary=ub' | csv-eval --binary=2ub --fields a,b --exit-if 'any(a != b)' --with-error 'input and output are not the same' | tail -c+41 | wc -c"
invert[1]="cat test.bin | cv-cat color-map=hot | cv-cat color-map=hot,invert | csv-paste '-;binary=ub' 'test.bin;binary=ub' | csv-eval --binary=2ub --fields a,b --exit-if 'any(a != b)' --with-error 'input and output are not the same' | tail -c+41 | wc -c"
